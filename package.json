{
  "name": "ocome",
  "version": "0.0.1",
  "description": "Desktop (React + Vite) and Mobile (React Native + Expo) monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": "9.4.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "pnpm --filter web start",
    "dev:mobile": "pnpm --filter mobile start",
    "build:web": "pnpm turbo run build --filter=web",
    "build:mobile": "pnpm turbo run build --filter=mobile",
    "build": "pnpm turbo run build",
    "test": "pnpm turbo run test",
    "lint": "pnpm turbo run lint",
    "tsc": "pnpm turbo run tsc"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.3",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-refresh": "^0.5.2",
    "globals": "^17.3.0",
    "prettier": "~3.5.3",
    "turbo": "^1.10.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.56.1",
    "vitest": "~4.0.18"
  }
}
